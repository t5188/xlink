{
  "log": {
    "access": "../log/access.log",
    "error": "../log/error.log",
    "loglevel": "debug",
    "dnsLog": true,
    "maskAddress": "half"
  },
  "inbounds": [
    {
      "listen": "::",
      "port": 1536,
      "protocol": "dokodemo-door",
      "sniffing": {
        "enabled": true,
        "destOverride": [
          "fakedns",
          "http",
          "tls",
          "quic"
        ],
        "routeOnly": false
      },
      "settings": {
        "network": "tcp,udp",
        "followRedirect": true
      },
      "streamSettings": {
        "sockopt": {
          "tproxy": "tproxy"
        }
      },
      "tag": "trp-in"
    },
    {
      "tag": "socks-in",
      "port": 10808,
      "listen": "127.0.0.1",
      "protocol": "socks",
      "sniffing": {
        "enabled": true,
        "destOverride": [
          "http",
          "tls",
          "fakedns"
        ],
        "routeOnly": false
      },
      "settings": {
        "auth": "noauth",
        "udp": true
      }
    }
  ]
}
